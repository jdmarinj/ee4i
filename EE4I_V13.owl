                    rdfs:range <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergySupply> ;
                    rdfs:comment "Relation linking the power delivery system or equipment to the Energy Supply parameter"@en ;
                    rdfs:label "deliversEnergy"@en .


###  https://w3id.org/ee4i#deliversEnergyType
ee4i:deliversEnergyType rdf:type owl:ObjectProperty ;
                        owl:inverseOf ee4i:energyTypeDeliveredBy ;
                        rdf:type owl:FunctionalProperty ;
                        rdfs:domain ee4i:PowerDeliveryUnit ;
                        rdfs:range <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergyType> ;
                        rdfs:comment "Relation linking the power delivery system or equipment to the Energy Type it delivers"@en ;
                        rdfs:label "deliversEnergyType"@en .


###  https://w3id.org/ee4i#energyDeliveredBy
ee4i:energyDeliveredBy rdf:type owl:ObjectProperty ;
                       rdfs:label "energyDeliveredBy"@en .


###  https://w3id.org/ee4i#energyStoredBy
ee4i:energyStoredBy rdf:type owl:ObjectProperty ;
                    owl:inverseOf ee4i:storesEnergy ;
                    rdfs:label "energyStoredBy"@en .


###  https://w3id.org/ee4i#energyTypeDeliveredBy
ee4i:energyTypeDeliveredBy rdf:type owl:ObjectProperty ,
                                    owl:InverseFunctionalProperty ;
                           rdfs:label "energyTypeDeliveredBy"@en .


###  https://w3id.org/ee4i#energyTypeStoredBy
ee4i:energyTypeStoredBy rdf:type owl:ObjectProperty ;
                        owl:inverseOf ee4i:storesEnergyType ;
                        rdfs:label "energyTypeStoredBy"@en .


###  https://w3id.org/ee4i#eventOf
ee4i:eventOf rdf:type owl:ObjectProperty ;
             rdfs:label "eventOf"@en .


###  https://w3id.org/ee4i#experiences
ee4i:experiences rdf:type owl:ObjectProperty ;
                 owl:inverseOf ee4i:isExperiencedBy ;
                 rdfs:domain ee4i:District ;
                 rdfs:range <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/gbBuildingOntology.owl#Weather> ;
                 rdfs:comment "Relation linking the District and the Weather it experiences. "@en ;
                 rdfs:label "experiences"@en .


###  https://w3id.org/ee4i#geoPointOf
ee4i:geoPointOf rdf:type owl:ObjectProperty ;
                rdfs:label "geoPointOf"@en .


###  https://w3id.org/ee4i#hasAirConditionedVolume
ee4i:hasAirConditionedVolume rdf:type owl:ObjectProperty ;
                             owl:inverseOf ee4i:isAirConditionedVolumeOf ;
                             rdf:type owl:FunctionalProperty ;
                             rdfs:domain <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/gbBuildingOntology.owl#Building> ;
                             rdfs:range <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/gbBuildingOntology.owl#Volume> ;
                             rdfs:comment "Relation linking a Building and its air conditioned Volume."@en ;
                             rdfs:label "hasAirConditionedVolume"@en .


###  https://w3id.org/ee4i#hasArea
ee4i:hasArea rdf:type owl:ObjectProperty ;
             owl:inverseOf ee4i:isAreaOf ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/gbBuildingOntology.owl#Building> ;
             rdfs:range <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/gbBuildingOntology.owl#Area> ;
             rdfs:comment "Relation linking Building and its constructed Area."@en ;
             rdfs:label "hasArea"@en .


###  https://w3id.org/ee4i#hasAssociatedKPI
ee4i:hasAssociatedKPI rdf:type owl:ObjectProperty ;
                      owl:inverseOf ee4i:isAssociatedKPIOf ;
                      rdfs:domain ee4i:PerformanceGoal ;
                      rdfs:range ee4i:KPI ;
                      rdfs:comment "The performance goal of stakeholder has associated key performance indicators to measure the progress towards the goal."@en ;
                      rdfs:label "hasAssociatedKPI"@en .


###  https://w3id.org/ee4i#hasAssociatedObject
ee4i:hasAssociatedObject rdf:type owl:ObjectProperty ;
                         owl:inverseOf ee4i:isAssociatedObjectOf ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain ee4i:KPICalculation ;
                         rdfs:range ee4i:KPIEvaluatedObject ;
                         rdfs:comment "To relate Key Performance Indicators with their evaluated objects."@en ;
                         rdfs:label "hasAssociatedObject"@en .


###  https://w3id.org/ee4i#hasBehavior
ee4i:hasBehavior rdf:type owl:ObjectProperty ;
                 owl:inverseOf ee4i:isBehaviorOf ;
                 rdfs:domain ee4i:Occupant ;
                 rdfs:range ee4i:OccupantBehavior ;
                 rdfs:comment "Relation linking an Occupant with its behavior (action to building energy systems)"@en ;
                 rdfs:label "hasBehavior"@en .


###  https://w3id.org/ee4i#hasBuildingType
ee4i:hasBuildingType rdf:type owl:ObjectProperty ;
                     owl:inverseOf ee4i:isBuildingTypeOf ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/gbBuildingOntology.owl#Building> ;
                     rdfs:range ee4i:BuildingType ;
                     rdfs:comment "Relation linking a Building and its functional type"@en ;
                     rdfs:label "hasBuildingType"@en .


###  https://w3id.org/ee4i#hasCalculation
ee4i:hasCalculation rdf:type owl:ObjectProperty ;
                    owl:inverseOf ee4i:isCalculationOf ;
                    rdf:type owl:InverseFunctionalProperty ;
                    rdfs:domain ee4i:KPI ;
                    rdfs:range ee4i:KPICalculation ;
                    rdfs:comment "To represent the calculation of key performance indicators"@en ;
                    rdfs:label "hasCalculation"@en .


###  https://w3id.org/ee4i#hasCalculationComponent
ee4i:hasCalculationComponent rdf:type owl:ObjectProperty ;
                             owl:inverseOf ee4i:isCalculationComponentOf ;
                             rdfs:domain ee4i:KPICalculation ;
                             rdfs:range ee4i:KPICalculationComponent ;
                             rdfs:comment "Relation linking the KPI Calculation and its needed Calculation Components"@en ;
                             rdfs:label "hasCalculationComponent"@en .


###  https://w3id.org/ee4i#hasCalculationModel
ee4i:hasCalculationModel rdf:type owl:ObjectProperty ;
                         owl:inverseOf ee4i:isCalculationModelOf ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain ee4i:KPICalculation ;
                         rdfs:range <http://identifiers.org/mamo/Mathematical_model> ;
                         rdfs:comment "To represent the mathematical model for KPI calculation"@en ;
                         rdfs:label "hasCalculationModel"@en .


###  https://w3id.org/ee4i#hasDeterminant
ee4i:hasDeterminant rdf:type owl:ObjectProperty ;
                    owl:inverseOf ee4i:isDeterminantOf ;
                    rdfs:domain <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergyCost> ;
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergyGain>
                                               <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergySupply>
                                               <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergyTariff>
                                             )
                               ] ;
                    rdfs:comment "Relation linking Energy Cost to its determinants like Energy Supply and Energy Tariff"@en ;
                    rdfs:label "hasDeterminant"@en .


###  https://w3id.org/ee4i#hasDisaggregation
ee4i:hasDisaggregation rdf:type owl:ObjectProperty ;
                       owl:inverseOf ee4i:isDisaggregationOf ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( ee4i:StrategicKPI
                                                   ee4i:TacticalKPI
                                                 )
                                   ] ;
                       rdfs:range [ rdf:type owl:Class ;
                                    owl:unionOf ( ee4i:OperationalKPI
                                                  ee4i:TacticalKPI
                                                )
                                  ] ;
                       rdfs:comment "Strategic KPI can be disaggregated to tactical KPI, and tactical KPI can be disaggregated to operational KPI."@en ;
                       rdfs:label "hasDisaggregation"@en .


###  https://w3id.org/ee4i#hasEquipmentParameter
ee4i:hasEquipmentParameter rdf:type owl:ObjectProperty ;
                           owl:inverseOf <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/gbBuildingOntology.owl#isEquipmentParameterOf> ;
