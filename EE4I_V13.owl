@prefix : <https://w3id.org/ee4i#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ssn: <http://purl.oclc.org/NET/ssnx/ssn> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ee4i: <https://w3id.org/ee4i#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix s4bldg: <https://w3id.org/def/saref4bldg#> .
@prefix s4inma: <https://w3id.org/def/saref4inma#> .
@base <https://w3id.org/ee4i#> .

<https://w3id.org/ee4i#> rdf:type owl:Ontology ;
                          owl:imports <https://saref.etsi.org/saref4inma/v1.1.2/> ;
                          dc:creator "Juan David Marín Jiménez" ,
                                     "Sandra Ximena Carvajal Quintero" ;
                          rdfs:comment "The targeted EE4I Ontology are aimed to exchange both master data and key performance information in industry for a multi-level energy performance analysis that addresses stakeholders’ performance goals. The targeted EE4I Ontology is to represent the key performance information. It is supposed to describe the definition of each KPI and their relationship, KPI calculation and needed datum source, the related performance goal and evaluated object. This subpart should provide the basis for energy performance tracking and assessment"@en ;
                          rdfs:label "EE4I Ontology, Energy Management - Key Performance Indicator Ontology"@en ;
                          owl:versionInfo "EE4I Ontology 1.1"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/identifier
dc:identifier rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
<http://purl.org/dc/terms/modified> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdfs:range xsd:string .


###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#isDefinedBy
rdfs:isDefinedBy rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#seeAlso
rdfs:seeAlso rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionInfo
owl:versionInfo rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#duration
xsd:duration rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.oclc.org/NET/ssnx/ssn#featureOfInterest
<http://purl.oclc.org/NET/ssnx/ssn#featureOfInterest> rdf:type owl:ObjectProperty ;
                                                      owl:inverseOf ee4i:isFeatureOfInterestOf ;
                                                      rdf:type owl:FunctionalProperty ;
                                                      rdfs:domain <http://purl.oclc.org/NET/ssnx/ssn#Observation> ;
                                                      rdfs:range <http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest> ;
                                                      rdfs:comment "Relation linking an Observation to its Feature Of Interest."@en ;
                                                      rdfs:label "featureOfInterest"@en .


###  http://purl.oclc.org/NET/ssnx/ssn#hasInput
<http://purl.oclc.org/NET/ssnx/ssn#hasInput> rdf:type owl:ObjectProperty ;
                                             owl:inverseOf ee4i:isInputOf ;
                                             rdfs:domain ee4i:KPICalculation ;
                                             rdfs:range ee4i:DatumSource ;
                                             rdfs:comment "The input of datum source for KPI calculation"@en ;
                                             rdfs:label "hasInput"@en .


###  http://purl.oclc.org/NET/ssnx/ssn#hasOutput
<http://purl.oclc.org/NET/ssnx/ssn#hasOutput> rdf:type owl:ObjectProperty ;
                                              owl:inverseOf ee4i:isOutputOf ;
                                              rdf:type owl:FunctionalProperty ;
                                              rdfs:domain ee4i:KPICalculation ;
                                              rdfs:range ee4i:KPIValue ;
                                              rdfs:comment "The output of KPI calculation, the calculation result of KPI"@en ;
                                              rdfs:label "hasOutput"@en .


###  http://purl.oclc.org/NET/ssnx/ssn#hasProperty
<http://purl.oclc.org/NET/ssnx/ssn#hasProperty> rdf:type owl:ObjectProperty ;
                                                owl:inverseOf <http://purl.oclc.org/NET/ssnx/ssn#isPropertyOf> ;
                                                rdf:type owl:InverseFunctionalProperty ;
                                                rdfs:comment "The chain here ensures that the observed property of an observation is a property of the feature of interest. "@en ;
                                                rdfs:label "hasProperty"@en .


###  http://purl.oclc.org/NET/ssnx/ssn#isPropertyOf
<http://purl.oclc.org/NET/ssnx/ssn#isPropertyOf> rdf:type owl:ObjectProperty ,
                                                          owl:FunctionalProperty ;
                                                 rdfs:domain <http://purl.oclc.org/NET/ssnx/ssn#Property> ;
                                                 rdfs:range <http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest> ;
                                                 rdfs:comment "Relation between a FeatureOfInterest and a Property (a Quality observable by a sensor) of that feature."^^xsd:string ;
                                                 rdfs:label "isPropertyOf"@en .


###  http://purl.oclc.org/NET/ssnx/ssn#observationResult
<http://purl.oclc.org/NET/ssnx/ssn#observationResult> rdf:type owl:ObjectProperty ;
                                                      owl:inverseOf ee4i:isObservationResultOf ;
                                                      rdf:type owl:InverseFunctionalProperty ;
                                                      rdfs:domain <http://purl.oclc.org/NET/ssnx/ssn#Observation> ;
                                                      rdfs:range <http://purl.oclc.org/NET/ssnx/ssn#ObservationValue> ;
                                                      rdfs:label "The result of an observation"@en ,
                                                                 "observationResult"@en .


###  http://purl.oclc.org/NET/ssnx/ssn#observationSamplingTime
<http://purl.oclc.org/NET/ssnx/ssn#observationSamplingTime> rdf:type owl:ObjectProperty ;
                                                            owl:inverseOf ee4i:isObservationSamplingTimeOf ;
                                                            rdf:type owl:FunctionalProperty ;
                                                            rdfs:domain <http://purl.oclc.org/NET/ssnx/ssn#Observation> ;
                                                            rdfs:range <http://www.w3.org/2006/time#Interval> ;
                                                            rdfs:comment "The time interval of an observation"@en ;
                                                            rdfs:label "observationSamplingTime"@en .


###  http://purl.oclc.org/NET/ssnx/ssn#observedProperty
<http://purl.oclc.org/NET/ssnx/ssn#observedProperty> rdf:type owl:ObjectProperty ;
                                                     owl:inverseOf ee4i:isObservedPropertyOf ;
                                                     rdf:type owl:FunctionalProperty ;
                                                     rdfs:domain <http://purl.oclc.org/NET/ssnx/ssn#Observation> ;
                                                     rdfs:range <http://purl.oclc.org/NET/ssnx/ssn#Property> ;
                                                     rdfs:comment "Relation linking an Observation to the Property that was observed. The observedProperty should be a Property (hasProperty) of the FeatureOfInterest (linked by featureOfInterest) of this observation."^^xsd:string ;
                                                     rdfs:label "observedProperty"@en .


###  http://schema.org/attendee
<http://schema.org/attendee> rdf:type owl:ObjectProperty ;
                             owl:inverseOf ee4i:attendeeOf ;
                             rdfs:domain <http://schema.org/Event> ;
                             rdfs:range ee4i:Occupant ;
                             rdfs:comment "Relation linking an Event and the Occupants who attend it"@en ;
                             rdfs:label "attendee"@en .


###  http://schema.org/event
<http://schema.org/event> rdf:type owl:ObjectProperty ;
                          owl:inverseOf ee4i:eventOf ;
                          rdfs:domain <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/gbBuildingOntology.owl#Zone> ;
                          rdfs:range <http://schema.org/Event> ;
                          rdfs:comment "Relation linking building Zone and its Event"@en ;
                          rdfs:label "event"@en .


###  http://schema.org/geo
<http://schema.org/geo> rdf:type owl:ObjectProperty ;
                        owl:inverseOf ee4i:geoPointOf ;
                        rdf:type owl:FunctionalProperty ;
                        rdfs:domain <http://purl.org/dc/terms/Location> ;
                        rdfs:range <http://www.w3.org/2003/01/geo/wgs84_pos#Point> ;
                        rdfs:comment "Relation linking a Location to geographical coordinates point. "@en ;
                        rdfs:label "geo"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasLocation
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasLocation> rdf:type owl:ObjectProperty ;
                                                                    owl:inverseOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isLocationOf> ;
                                                                    rdf:type owl:FunctionalProperty ;
                                                                    rdfs:domain [ rdf:type owl:Class ;
                                                                                  owl:unionOf ( <http://schema.org/Event>
                                                                                                <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject>
                                                                                                ee4i:District
                                                                                              )
                                                                                ] ;
                                                                    rdfs:range <http://purl.org/dc/terms/Location> ;
                                                                    rdfs:comment "Relation linking an object or event to a geographical Location."@en ;
                                                                    rdfs:label "hasLocation"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isLocationOf
<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isLocationOf> rdf:type owl:ObjectProperty ,
                                                                              owl:InverseFunctionalProperty ;
                                                                     rdfs:domain <http://purl.org/dc/terms/Location> ;
                                                                     rdfs:label "isLocationOf"@en .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdf:type owl:ObjectProperty .


###  http://www.w3.org/2006/time#hasBeginning
<http://www.w3.org/2006/time#hasBeginning> rdf:type owl:ObjectProperty ;
                                           owl:inverseOf ee4i:isBeginningOf ;
                                           rdf:type owl:FunctionalProperty ;
                                           rdfs:domain <http://www.w3.org/2006/time#Interval> ;
                                           rdfs:range <http://www.w3.org/2006/time#Instant> ;
                                           rdfs:comment "Relation linking an Interval to its beginning Instant"@en ;
                                           rdfs:label "hasBeginning"@en .


###  http://www.w3.org/2006/time#hasEnd
<http://www.w3.org/2006/time#hasEnd> rdf:type owl:ObjectProperty ;
                                     owl:inverseOf ee4i:isEndOf ;
                                     rdf:type owl:FunctionalProperty ;
                                     rdfs:domain <http://www.w3.org/2006/time#Interval> ;
                                     rdfs:range <http://www.w3.org/2006/time#Instant> ;
                                     rdfs:comment "Relation linking an Interval to its end Instant"@en ;
                                     rdfs:label "hasEnd"@en .


###  https://w3id.org/ee4i#actsTo
ee4i:actsTo rdf:type owl:ObjectProperty ;
            owl:inverseOf ee4i:isActedBy ;
            rdfs:domain ee4i:OccupantBehavior ;
            rdfs:range <https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergyFacility> ;
            rdfs:comment "Relation linking the Occupant Behavior and the Energy Consumer Faility it acts"@en ;
            rdfs:label "actsTo"@en .


###  https://w3id.org/ee4i#attendeeOf
ee4i:attendeeOf rdf:type owl:ObjectProperty ;
                rdfs:label "attendeeOf"@en .


###  https://w3id.org/ee4i#composes
ee4i:composes rdf:type owl:ObjectProperty ;
              owl:inverseOf ee4i:isComposedBy ;
              rdfs:domain ee4i:PowerEquipment ;
              rdfs:range ee4i:EquipmentContainer ;
